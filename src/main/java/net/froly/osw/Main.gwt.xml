<module rename-to="Main">

    <!-- Inherit the core Web Toolkit stuff.                  -->
    <inherits name='com.google.gwt.user.User'/>

    <set-property name="user.agent" value="safari"/>
    
    <stylesheet src="jqtouch/jqtouch.css" />
    <stylesheet src="themes/jqt/theme.css?v=2"/>
    <stylesheet src="jqtext/scrolling.css"/>
    <stylesheet src="osw.css?v=6"/>

    <script src="jqtouch/jquery-1.4.2.min.js"/>
    <script src="jqtouch/jqtouch.js"/>
    <script src="jqtext/jqt.scroll.js"/>
    
    <!-- Specify the app entry point class.                   -->
    <entry-point class='net.froly.osw.client.OswClient'/>


    <servlet path="/ActivityService" class="net.froly.osw.server.model.ActivityServiceImpl"/>
    <servlet path="/ContactService" class="net.froly.osw.server.model.ContactServiceImpl"/>
</module>